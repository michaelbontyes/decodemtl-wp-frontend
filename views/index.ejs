<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <%-title%>
    <%-meta%>
    <% if (typeof appCss !== 'undefined') {%>
      <link rel="stylesheet" href="<%=appCss%>">
    <%}%>
  </head>
  <body>
    <div id="app"><%- typeof html === 'undefined' ? '' : html %></div>
    <script>
      window.__reduxInitialState = <%-JSON.stringify(store.getState()) %>;
    </script>
    <% if (typeof appJs !== 'undefined') {%>
      <script src="<%=appJs%>"></script>
    <%}%>
  </body>
</html>
